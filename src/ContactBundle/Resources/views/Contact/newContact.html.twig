{% extends 'ContactBundle::layout.html.twig' %}
{% block body %}	
	{% block content %}	
		<h1>Contact Form</h1>
		{% if (form is not null) %}
			<form action="{{ url('api_1_post_contact') }}" method="POST" {{form_enctype(form) }}>

				{{ form_widget(form) }}	

				<input type="submit" value="submit">

				{{ form_end(form) }}
				
			</form>
		{% endif %}
		<a href = "{{ path('api_1_get_contacts') }}"> Go Back</a>
	{% endblock %}
{% endblock %}