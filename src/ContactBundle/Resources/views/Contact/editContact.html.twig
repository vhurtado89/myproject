{% block content %}	
	<h1>Contact Form</h1>
	{% if (form is not null) %}
		<form action="{{ url('api_1_put_contact', {'id': id}) }}" method="POST" {{ form_enctype(form) }}>

			{{ form_widget(form) }}		
				
			<input type="submit" value="submit">
			
			{{ form_end(form) }}
		</form>
	{% endif %}
{% endblock %}